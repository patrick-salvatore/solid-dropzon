{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "newLine": "LF",
    "outDir": "./dist",
    "declaration": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictNullChecks": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["solid-js"]
  },
  "include": ["./src"],
  "exclude": ["node_modules"]
}
